{
    "name": "ergontech/simpleproductalert",
    "type": "magento-module",
    "description": "Allow customers to subscribe to associated simple products of a visible configurable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ErgonTech/MageTest_Utils"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "aydin-hassan/magento-core-composer-installer": "^1.4",
        "magento/core": "^1.9",
        "magetest/magento-phpunit-extension": "dev-develop",
        "ergontech/magetest-utils": "dev-master"
    },
    "extra": {
        "map": [
            [
                "app/etc/modules/ErgonTech_SimpleProductAlert.xml",
                "app/etc/modules/ErgonTech_SimpleProductAlert.xml"
            ],
            [
                "app/code/community/ErgonTech/SimpleProductAlerts/",
                "app/code/community/ErgonTech/SimpleProductAlerts/"
            ],
            [
                "design/frontend/base/default/layout/ergontech_simpleproductalert.xml",
                "design/frontend/base/default/layout/ergontech_simpleproductalert.xml"
            ]
        ],
        "magento-root-dir": "root"
    },
    "autoload-dev": {
        "files": [
            "root/app/Mage.php"
        ],
        "psr-0": {
            "": [
                "app/code/local",
                "app/code/community",
                "root/app/code/community",
                "root/app/code/core",
                "root/lib"
            ]
        }
    }
}